{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Docker: Attach to Node",
      "localRoot": "${workspaceFolder}/src",
      "remoteRoot": "/app",
      "address": "localhost",
      "port": 9229,
      "skipFiles": [
        "<node_internals>/app/**/*.js",
        "${workspaceFolder}/src/node_modules/**/*.js"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}
